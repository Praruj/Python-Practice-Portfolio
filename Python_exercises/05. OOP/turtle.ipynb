   "id": "098aca59",
   "metadata": {},
   "outputs": [],
   "source": [
    "from turtle import *\n",
    "t1=Turtle()\n",
    "t2=Turtle()\n",
    "t1.color(\"red\")\n",
    "t2.color(\"blue\")\n",
    "for i in range(180):\n",
    "    t1.forward(5)\n",
    "    t2.forward(3)\n",
    "    t1.left(2)\n",
    "    t2.left(2)\n",
    "done()"
   ]
  }
